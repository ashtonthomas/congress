%form{:role => "form"}
  .form-group
    %label{:for => "exampleInputEmail1"} Email address
    %input#exampleInputEmail1.form-control{:placeholder => "Enter email", :type => "email"}/
  .form-group
    %label{:for => "exampleInputPassword1"} First Name
    %input#exampleInputFirstName.form-control{:placeholder => "First Name", :type => "first_name"}/
  .form-group
    %label{:for => "exampleInputPassword1"} Last Name
    %input#exampleInputLastName.form-control{:placeholder => "Last Name", :type => "last_name"}/
  .form-group
    %label{:for => "exampleInputPassword1"} Password
    %input#exampleInputPassword1.form-control{:placeholder => "Password", :type => "password"}/
  .form-group
    %label{:for => "exampleInputPasswordConf"} Confirm Password
    %input#exampleInputPassword1.form-control{:placeholder => "Confirm Password", :type => "password"}/
  %button.btn.btn-default{:type => "submit"} Submit
